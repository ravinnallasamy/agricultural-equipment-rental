{
  "name": "agricultural-equipment-rental-frontend",
  "version": "1.0.0",
  "description": "Frontend application for Agricultural Equipment Rental Platform",
  "private": false,
  "homepage": ".",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^4.10.1",
    "react-router-dom": "^7.8.0",
    "react-scripts": "5.0.1",
